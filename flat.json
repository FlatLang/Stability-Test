{
  "name": "StabilityTest",
  "version": "1.0.0",
  "description": "Stability test for Flat",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "js",
  "scripts": {
    "test": "node dist/js/test.js"
  },
  "sources": {
    "browser": {
      "mainClass": "stabilitytest/BrowserStabilityTest",
      "source": "browser/src",
      "excluded": ["src"],
      "bin": "browser-stability-test",
      "outFile": "browser-stability-test.js",
      "dependencies": {
        "github:FlatLang/FuCli": "master",
        "github:FlatLang/System": "master",
        "github:FlatLang/IO": "master",
        "github:FlatLang/Selenium": "master"
      }
    },
    "test": {
      "mainClass": "stabilitytest/StabilityTest",
      "source": "src",
      "dependencies": {
        "github:FlatLang/Standard-Library": "master",
        "github:FlatLang/Test": "master",
        "github:FlatLang/System": "master",
        "github:FlatLang/IO": "master",
        "github:FlatLang/Network": "master",
        "github:FlatLang/Redis": "master"
      }
    }
  }
}
